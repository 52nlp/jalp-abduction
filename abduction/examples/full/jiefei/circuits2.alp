output(Gate, Value) :-
	inverter(Gate),
	input(Gate,InValue),
	opposite(InValue,Value),
	not broken(Gate).

output(Gate, InValue) :-
	inverter(Gate),
	input(Gate,InValue),
	broken(Gate).

input(g2,Value):- output(g1,Value).
input(g3,Value):- output(g2,Value).
